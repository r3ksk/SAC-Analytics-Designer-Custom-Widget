{
	"eula": "",
	"vendor": "Raghu",
	"license": "",
	"id": "Raghu",
	"version": "0.1.0",
	"name": "Hello World Widget",
	"newInstancePrefix": "Hello World Widget",
	"description": "A Hello World Custom Widget ",
	"icon": "",
	"webcomponents": [
		{
			"kind": "main",
			"tag": "com-sap-helloWorld",
			"url": "https://r3ksk.github.io/SAC-Analytics-Designer-Custom-Widget/HelloWorld-Widget/release/0.1.0/helloWorldSACWidget.js",
			"integrity": "",
			"ignoreIntegrity": true
		}
	],
	"properties": {
		"name": {
			"type": "string",
            "description": "input user name",
			"default": "Raghuraman"
		},
		"message": {
			"type": "string",
            "description": "HW Message",
			"default": "Hello World Raghuraman"
		}
	},
	"methods": {
		"sayHello": {
			"description": "Method to say Hello",
			"parameters": [],
			"body": "this.message = 'Hello World ' + this.name;"
		},
		"setName": {
			"description": "Sets Name attribute in the Hello World Widget",
			"parameters": [
				{
					"name": "n",
					"type": "string"
				}
			],
			"body": "this.name = n;"
		},
		"getName": {
			"returnType": "string",
			"description": "Gets Name attribute in the Hello World Widget",
			"parameters": [],
			"body": "return this.message;"
		}
	},
	"events": {}
}